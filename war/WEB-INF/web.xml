<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
	<servlet>
		<servlet-name>feedbackServlet</servlet-name>
		<servlet-class>fraengerador.server.FeedbackServiceImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>feedbackServlet</servlet-name>
		<url-pattern>/fraengerador/feedback</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>Fraengerador.html</welcome-file>
	</welcome-file-list>
</web-app>
